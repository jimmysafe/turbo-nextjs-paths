{
    "$schema": "https://turborepo.org/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ],
            "cache": false
        },
        "clean": {
            "cache": false
        },
        "dev": {
            "cache": false,
            "dependsOn": [
                "^build"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ],
            "cache": false,
            "persistent": false
        },
        "start": {
            "cache": false
        },
        "test": {
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "type-check": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "**/node_modules/.cache/tsbuildinfo.json"
            ]
        }
    }
}